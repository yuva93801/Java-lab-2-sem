function method1(){
const str="Hello RVCE";
console.log("length of string:-> ",str.length);
console.log("index of 'R':-> ",str.indexOf('R'));
console.log("UPPER os string:-> ",str.toUpperCase());
console.log("substring:-> ",str.substring(6,10));
}

method1()
function method2(){

const dt=new Date();
console.log("Year:-> ",dt.getFullYear());
console.log("Month:-> ",dt.getMonth()+1);
console.log("Date:-> ",dt);
console.log("Date as a String:-> ",dt.toDateString());
}
method2()

function method3(){
const num=186.9654;
console.log("fixed number decimals:-> ",num.toFixed(3));
console.log("precision:-> ",num.toPrecision(2));
console.log("to string:-> ",num.toString());
const num1="182.2";
console.log("to int:-> ", parseInt(num1));
}
method3()
